<html>
	<head>
		<link href="http://netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css" rel="stylesheet">	
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />	
	</head>
	<body>
	<script type="text/javascript">
	function onResizeChange(){
	var range = document.getElementById("resize").value;
	var output = document.getElementById("rangeoutput");
	output.innerHTML = range + "";
	}
	function onRotateChange(){
	var range = document.getElementById("rotate").value;
	var output = document.getElementById("rotateoutput");
	output.innerHTML = range + "";
	}
	function onBlurChange(){
	var range = document.getElementById("blurvalue").value;
	var output = document.getElementById("bluroutput");
	output.innerHTML = range + "";
	}
	</script>
		<nav class="navbar">	
		</nav>
		<div class="container">
		<section id="section1" class="panel panel-default">
			<div class="panel-heading">
			<div class="container">
	    		<h3 class="panel-title">Graphics editor</h3>
			</div>
		  	</div>
	  		<div class="panel-body">
				<form action="http://mariusz.matusiak.s3.amazonaws.com/" method="POST" id="sign-up-form" class="form-horizontal" enctype="multipart/form-data">
					<div class="container">
						<div class="col-xs-3">
						<% for(var i=0; i<fields.length; i++){%>
						<input type="hidden" name="<%=fields[i].name %>" value="<%=fields[i].value %>" />
						<% } %>
							<input type="file" name="file" accept="image/*" />
						</div><br><br>
					</div>
				<div class="container">
				<div class="col-xs-3">
					<input type="submit" class="btn  btn-primary btn-block" value="Confirm"/>
				</div>
				</div>
				</form>
			</div>
			<footer class="panel-footer">
			<a href="/files">Browse files</a>
			</footer>
		</section>
	</div>		
	</body>
</html>


